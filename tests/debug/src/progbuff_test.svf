// Check jtag id
SDR 32 TDO(1002ac05);
// Activating DM
SIR 5 TDI(11);
SDR 41 TDI(4000000006);
// Read DMStatus, compare bottom 8 bits to 0xa3
SDR 41 TDI(4400000001);
SDR 41 TDO(28c) MASK(3ff);
// Reset hart
SDR 41 TDI(400000000e);
SDR 41 TDI(4000000006);
SDR 41 TDI(4040000006);
// Halt hart
SDR 41 TDI(4200000006);
SDR 41 TDI(4400000001);
SDR 41 TDO(c00) MASK(c03);
// Write "0x80000000" to x8
SDR 41 TDI(1200000002);
SDR 41 TDI(1400000002);
SDR 41 TDI(5c00cc4022);
RUNTEST 64;
// Write "0xbadc0ffee0ddf00d" to x9
SDR 41 TDI(138377c036);
SDR 41 TDI(16eb703ffa);
SDR 41 TDI(5c00cc4026);
RUNTEST 64;
// Write program buffer:
SDR 41 TDI(800250c08e);
SDR 41 TDI(840210104e);
SDR 41 TDI(88004001ce);
// Execute Program Buffer
SDR 41 TDI(5c00100002);
RUNTEST 10;
